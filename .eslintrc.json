{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "mocha": true
    },
    "extends": [
        "google",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": ["off"],
        "complexity": ["error", {"max": 5}],
        "require-jsdoc": ["off"],
        "new-cap": ["off"]
    }
}
